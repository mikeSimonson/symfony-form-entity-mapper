{
    "name": "mikesimonson/symfony-form-value-object-mapper",
    "description": "A library that can instantiate automatically value object mapped by Symfony forms despite having a constructor with arguments.",
    "type": "library",
    "require": {
        "symfony/form": "2.3.*",
        "zendframework/zend-code": "^2.2.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "MikeSimonson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MikeSimonson\\": "tests"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mike",
            "email": "mike.simonson@gmail.com",
            "homepage": "http://mikesimonson.com"
        }
    ]
}
